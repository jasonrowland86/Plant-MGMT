<div class="container">
  <div class="user-content">
    <div>
      <h2>Dry Adapted</h2>
      <% @plants.each do |plant| %>
        <div class="plant">
          <%= link_to plant.name, plant_path(plant) %>
        </div>
      <% end %>
      <br>

      <%= link_to 'Add New Plant', new_plant_path %>
      <br>
      <%= link_to 'Back', plants_path %>
    </div>
  </div>
</div>
