<div class="container">
  <div class="user-content">
    <h2>Create Account</h2>

    <%= form_for @user do |f| %>
      <%= f.text_field :username, placeholder: 'Username' %>
      <%= f.password_field :password, placeholder: 'Password' %>

      <%= f.submit 'Register' %>
    <% end %>
  </div>
</div>
